# OpenAI API Key (REQUIRED for chatbot)
OPENAI_API_KEY=your_openai_api_key_here

# Venue Search APIs (Choose ONE - all have free tiers)
# Priority: Foursquare > Google Places > Yelp

# Foursquare API (RECOMMENDED - FREE: 950 calls/day)
# Get key at: https://foursquare.com/developers/signup
FOURSQUARE_API_KEY=your_foursquare_api_key_here

# Google Places API (FREE: 5000 requests/month)
# Get key at: https://console.cloud.google.com/apis/credentials
GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# Yelp Fusion API (PAID - only use if you have access)
# Get key at: https://www.yelp.com/developers
YELP_API_KEY=your_yelp_api_key_here
